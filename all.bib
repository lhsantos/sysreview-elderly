 keyword = {assisted living;geriatrics;ubiquitous computing;Alzheimer type condition;context processing;contextual information;dementia;demographic change;demographic profile;elderly population;healthcare implication;independent assisted living;intelligent context aware systems;negative behavioral symptom;Computational modeling;Context;Dementia;Knowledge engineering;Semiotics;Unified modeling language;Kansei engineering;assisted living;conceptual models;dementia;intelligent context-aware systems;knowledge;sematics;semiotics},
 note = {from: ieee.bib},
 pages = {1180--1185},
 title = {Towards Independent Assisted Living: The Application of Intelligent Context-Aware Systems},

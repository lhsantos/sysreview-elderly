@inproceedings{7333601,
 abstract = {Research in robotics has made available numerous possibilities and tools for further innovation in the psychological practice. For instance, recent research provided many examples of possible applications of robots in the education and rehabilitation of people with learning difficulties and/or intellectual disabilities. In this paper, we present a study on how cultural backgrounds can influence the perception and intention to use a robot as an instrument in the future practice. The study involved 37 Italian students and 37 UK students, as future professionals in the field of psychology, which experienced the actual capabilities of a humanoid robot through a live demo. In this work, we explored the main factors of the Unified Theory of Acceptance and Use of Technology (UTAUT) with the aim to reveal cultural differences. The instrument used was the UTAUT questionnaire, which was designed and validated to investigate the robot acceptance and use. A significant difference on the intention to use the robot is reported in our results. The discriminant analysis produced a very high degree of separation between the two groups, confirming that there is a different approach toward the use of robotics between the two cultures.},
 author = {Conti, D. and Cattani, A. and Nuovo, S. Di and Nuovo, A. Di},

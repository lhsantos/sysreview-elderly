 doi = {10.1109/UIC-ATC.2013.54},
 keyword = {Bluetooth;Global Positioning System;alarm systems;assisted living;geriatrics;mobile computing;smart phones;3-axial accelerator;ADL;Bluetooth;GPS location;activities of daily living;alerting system;angular velocity;autonomous fall detection;custom vest;elderly;emergent text message;global positioning system;gyroscope;health care center;mobile computing;mobile smart phone;threshold-based fall detection algorithm;ubiquitous computing;Acceleration;Accelerometers;Angular velocity;Gyroscopes;Mobile communication;Senior citizens;Smart phones;Bluetooth;Fall Detection;Mobile Smart Phone;Ubiquitous Computing},
 note = {from: ieee.bib},

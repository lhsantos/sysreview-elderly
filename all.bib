 keyword = {geriatrics;health care;human factors;image retrieval;recommender systems;ubiquitous computing;CARE prototype system;active recommender mode;digital image frame;digital picture frame;elderly people well-being enhancement;log file data analysis;sensors;system usage;user appreciation;user environment;well-being model;Context;Interviews;Mobile handsets;Prototypes;Senior citizens;Sensors;Usability;Well-being and lifestyle support;acceptability;pervasive healthcare for the elderly;usability},
 note = {from: ieee.bib},
 pages = {112--120},
 title = {CARE - extending a digital picture frame with a recommender mode to enhance well-being of elderly people},
 year = {2015}
}

@inproceedings{7349387,
 abstract = {While many aging in place technologies have been explored in the literature, few have focused on low socioeconomic status (SES) populations. Contextual observations (n=8) were used to gain an in-depth view of the daily needs and challenges of low SES older adults and to provide insights into their experiences of health in and around the home. Our findings have implications not only for the kinds of technologies this population could benefit from, but the way in which such technologies are designed to fit into their current lifestyle.},

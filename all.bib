 keyword = {Internet;behavioural sciences computing;geriatrics;health care;Web-based community;Web-based prognostics scoring system;aged day-care service;attendance rate;day-cared treatment;emotion scale;emotional needs;ethical care;positive attitude;practical care support exercise;self-esteem issue;social welfare;Aging;Context-aware services;Costs;Feeds;Frequency;Medical services;Medical treatment;Prototypes;Senior citizens;Technological innovation;Aged Day-Care Service (ADCS);Healthcare service},
 note = {from: ieee.bib},

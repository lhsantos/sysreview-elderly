@inproceedings{5976170,
 abstract = {Recently, opportunities that a consumer has to make his/her choices are increasing. And the need of the remote consultation is rising as a means of making better choices efficiently. However, the burden of the expert (consultant) is getting large in the existing remote consultation, because there is a large difference between the number of customers and that of experts. Therefore, it is difficult to carry out a consultation that considers the customer's personality and personal preferences and that matches well to the customer's needs. In this paper, we propose a technique to solve these problems by applying life log for basic data acquisition in remote consultation and present useful application examples.},

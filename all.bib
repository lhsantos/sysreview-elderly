@inproceedings{7349388,
 abstract = {Smart homes have significant potential to enhance the lives of older adults, extending the period of healthy ageing, through monitoring wellbeing, detecting decline and applying interventions to prevent or slow down this decline. In this paper we present results from interviews with 7 older adults who have been living in smart homes for over 4 years. Our aims were to 1) examine attitudes to living with sensors and AAL technology over time; 2) gather opinions on the usefulness of this data for supporting self-management of health and wellbeing and 3) evaluate the effectiveness of various visualization techniques for presenting sensor-based health and wellness data. Our findings show that older adults are interested in receiving feedback from sensor technology to support them self-managing their wellbeing. Potential beneficial information includes time spent inside and outside the home, walking time, sleep, activity, blood pressure and weight. This information needs to be enhanced by education and goal-setting and by representing data using visualisations that are simple and intuitive.},
 author = {Doyle, J. and Caprani, N. and Bond, R.},
 booktitle = {Pervasive Computing Technologies for Healthcare (PervasiveHealth), 2015 9th International Conference on},
 doi = {10.4108/icst.pervasivehealth.2015.259279},
 keyword = {assisted living;geriatrics;human factors;sensors;AAL technology;data representation;health and wellness self-management;older adult attitudes;older adult lives;sensor technology;sensor-based health data;sensor-based wellness data;smart home data;visualization techniques;wellbeing monitoring;Biomedical monitoring;Data visualization;Interviews;Medical services;Monitoring;Sensors;Smart homes;Older adults;smart home data;wellness self-management},
 note = {from: ieee.bib},
 pages = {129--136},
 title = {Older adults' attitudes to self-management of health and wellness through smart home data},
 year = {2015}
}

@inproceedings{7349389,
 abstract = {Recent cognitive decline screening batteries have highlighted the importance of language deficits related to semantic knowledge breakdown to reveal the incipient dementia. This paper proposes the introduction of novel enriched linguistic tests and examines the hypothesis that language can be a sensitive cognitive measure for Mild Cognitive Impairment (MCI). A group of MCI and healthy elderly were administered a set of proposed linguistic tests. Performance measures were made on both groups to indicate that concrete verbal production deficits such as impaired verb fluency can distinguish the MCI from normal aging. In addition, it was found that even in cases where the MCI subjects preserved scores, language tests took significantly more time compared to healthy controls. These findings indicate that language could be a sensitive cognitive marker in preclinical stages of MCI.},
 author = {Segkouli, S. and Paliokas, I. and Tzovaras, D. and Giakoumis, D. and Karagiannidis, C.},
 booktitle = {Pervasive Computing Technologies for Healthcare (PervasiveHealth), 2015 9th International Conference on},
 doi = {10.4108/icst.pervasivehealth.2015.258945},
 keyword = {cognition;handicapped aids;linguistics;MCI;cognitive decline screening battery;incipient dementia;language ability;linguistic test;mild cognitive impairment;semantic knowledge breakdown;speech ability;Aging;Batteries;Education;Pragmatics;Production;Semantics;Senior citizens;Linguistic Test;Mild Cognitive Impairment;Screening batteries;Verbal fluency},
 note = {from: ieee.bib},
 pages = {137--140},
 title = {Design of novel screening environments for Mild Cognitive Impairment: Giving priority to elicited speech and language abilities},
 year = {2015}
}

@inproceedings{7349399,
 abstract = {Social Networking Sites have become useful tools to strengthen communication with family and friends. Older adults, however, are often hesitant to use SNSs and are reluctant adopters of modern communication media. This results in an asymmetric condition, where younger and older adults use different media and communication channels. To balance communication practices we integrated classic media based on pen and paper with online social networks. In this paper we present a 36 weeks deployment study with an extended family (n=12) of the use of Tlatosketch, a hybrid system based on a digital photo frame, and digital pen and paper technology. This combination allows older adults to stay updated on Facebook and to post paper notes to Facebook. Our results show that Tlatosketch provides the older adult with a new media for self-expression, and improves the family social experience, thus helping them remain active while ageing. We close discussing directions for future work.},
 author = {Cornejo, R. and Weibel, N. and Tentori, M. and Favela, J.},
 booktitle = {Pervasive Computing Technologies for Healthcare (PervasiveHealth), 2015 9th International Conference on},
 doi = {10.4108/icst.pervasivehealth.2015.259069},
 keyword = {assisted living;human computer interaction;human factors;social networking (online);Facebook;Tlatosketch;active aging;asymmetric condition;communication channels;communication media;digital pen and paper technology;digital photo frame based hybrid system;media channels;older adults;online social networks;paper notes;paper-based SNS application;self-expression;social networking sites;younger adults;Aging;Communication channels;Facebook;Interviews;Media;Postal services;Older adults;active aging;digital pen and paper;social networking sites},
 note = {from: ieee.bib},
 pages = {209--212},
 title = {Promoting active aging with a paper-based SNS application},
 year = {2015}
}

@inproceedings{7349401,
 abstract = {We designed a tablet game to diagnose, train, and assess the rehabilitative progress of hemi-spatial neglect patients. We found that a set of parameters from fitting Fitts' law to hit delays in a whack-a-mole game can be used to identify neglect patients due to the asymmetry between performance on the left and right hand sides of the screen. Performance improvements were evident in in-game metrics as well as to the patients through summary screens.},
 author = {Knoche, H. and Hald, K. and Tamsen, D. and Jespersen, L. H.},

@inproceedings{4055150,
 abstract = {Drinking plays a role in the social activities of University of Virginia (UVA) students, including the annual Foxfield Horse Races. However, no data gauge the levels of binge, or high-risk, drinking at Foxfield. A Web-based survey has been developed to begin to measure the drinking norms at Foxfield with the hopes of using the data to create a safer environment at future events. Using a limited sample of 78 students, the preliminary data show that students drink more at Foxfield than each day of a normal week. Students also report drinking more and longer at Foxfield than during Halloween and football games (events known to be large social events at UVA). 66% of respondents also reported at least one negative consequence as a result of another's drinking and 68% reported at least one negative consequence as a result of his own drinking. This work helps to lay the groundwork for a large sample data collection after the 2006 running of the Foxfield Races on April 29th},
 author = {Guha, N. and Bass, E. J. and Bruce, S. E.},
 booktitle = {2006 IEEE Systems and Information Engineering Design Symposium},
 doi = {10.1109/SIEDS.2006.278688},

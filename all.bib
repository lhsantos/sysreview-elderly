 keyword = {CMOS image sensors;object detection;radar detection;ultra wideband communication;wireless sensor networks;Doppler shift;LOS;RF sensor;cardiopulmonary activity;continuous wave unmodulated waveform;environmental noise;human feature detection;line of sight;microDoppler technique;radiofrequency sensor;short-range UWB impulse-radio CMOS sensor;wireless infrastructure;wireless noncontact sensor;Arrays;CMOS integrated circuits;Delay;Humans;Monitoring;Ultra wideband radar},
 note = {from: ieee.bib},

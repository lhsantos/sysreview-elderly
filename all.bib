 keyword = {assisted living;bending;biomedical transducers;cardiology;digital filters;fibre optic sensors;medical signal detection;microsensors;patient monitoring;ambient application;ambient assistive living;ballistocardiographic signal;data collection;digital filter;healthcare application;information processing;microbending fiber optic sensor data;noise removal;optical transducer;signal data processing;signal filtering;vital sign extraction;vital sign monitoring;vital sign waveform;Electrocardiography;Error analysis;Filtering algorithms;Heart rate;Monitoring;Optical fiber filters;Optical fiber sensors;fiber optic sensor;health and well being;signal processing;vital signs monitoring},
 note = {from: ieee.bib},
 pages = {1--6},
 title = {Information processing of optical sensor data in ambient applications},
 year = {2014}
}

@inproceedings{6827735,
 abstract = {With the upcoming aging society and emerging of some newly discovered chronic diseases, the demand of hospital nursing for elderly patients had been significantly increased. It is a critical issue for health worker to provide a comprehensive, proactive health care in hospital, especially for those vulnerable patients who are unable to speak or behave for themselves. This study proposes an innovative, wearable RFID tag which embedded body temperature monitoring sensor, and equipped with automatic identification and localization, real-time emergency notification for health care workers. Additionally, operating with the intelligent backend system architecture, this system can also provide immediate physician advices in case if emergency situation happens without doctor near the site. The result of this study provides a ubiquitous medical care throughout whole hospital, and the new-invented tag may bring a significant change to normal health care process, especially in patient care.},

 keyword = {fuzzy logic;health care;home computing;patient monitoring;sensor fusion;telemedicine;ubiquitous computing;acoustical environment;automatic in-home healthcare monitoring system;data sets;environmental condition;environmental disturbance;fuzzy logic fusion method;medical in-home telemonitoring;medical knowledge;medical recommendation;multimodal fusion;multimodal platform;pervasive in-home health monitoring;pervasive multisensor data fusion;smart home healthcare monitoring;telemonitoring platform;Biomedical monitoring;Fuzzy logic;Monitoring;Senior citizens;Sensor systems;fuzzy control;fuzzy logic;healthcare telemonitoring;multimodal data fusion;telemedicine},
 note = {from: ieee.bib},

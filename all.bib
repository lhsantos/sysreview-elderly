 keyword = {geriatrics;interactive devices;medical computing;patient treatment;Barthel;Berg scale;MMSe;Nintendo Wii;SF-36;Wiitherapy;aging population;depressive effect;geriatric gymnastics;mental domains;physical domains;physiotherapist supervision;senior population;therapeutic instrument;Diseases;Educational institutions;Games;Geriatrics;Senior citizens;Geriatric Gymnastics;Institutionalize;Nintendo Wii®;Older Adult;Physiotherapy;Wiitherapy},
 note = {from: ieee.bib},

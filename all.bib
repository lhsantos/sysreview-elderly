 keyword = {assisted living;cellular radio;diseases;geriatrics;intelligent sensors;telemedicine;wearable computers;GSM module;WeCare;automatic danger detecting and alerting tool;chronic disease;elderly danger detection and alert;energy efficient accelerometer;health threatening conditions;heart attack;intelligent badge;medical attention;service center;wearable badge;Acceleration;Elevators;GSM;Senior citizens;Sensitivity;Sensors;danger detection;elderly healthcare;fall detection;unexpected behavior},
 note = {from: ieee.bib},

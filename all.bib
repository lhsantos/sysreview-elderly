 issn = {1083-4427},
 journal = {IEEE Transactions on Systems, Man, and Cybernetics - Part A: Systems and Humans},
 keyword = {data privacy;meta data;ubiquitous computing;asymmetric trust relationships;metadata;pervasive computing;privacy preserving data dissemination;private information;sensitive data;Collaboration;Computer science;Control systems;Data privacy;Intelligent systems;Medical services;Pervasive computing;Protection;Senior members;Web and internet services;Data dissemination;data privacy protection;privacy;trust},
 note = {from: ieee.bib},

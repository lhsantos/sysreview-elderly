 note = {from: ieee.bib},
 pages = {36--41},
 title = {Mobile grid computing for data- and patient-centric ubiquitous healthcare},
 year = {2012}
}

@inproceedings{6314562,
 abstract = {Many neurodegenerative disorders affect cognitive and memory functions. This paper presents work in progress of a tool which helps in the treatment of such disorders. To achieve the platform, a multidisciplinary development team was cooperated with. The proposed system involves users in different situations by means of exercises. It is demonstrated that if the person works the cognitive area, he/she can improve. Therefore, with the development of these exercises, users train memory, understanding and social skills. Moreover, doctors can check the evolution of their patients due to the fact that the system stores objective information during the exercises.},

 keyword = {cloud computing;computerised monitoring;data analysis;electronic health records;geriatrics;health care;mobile computing;assignment module;cloud computing platform;cloud data storage module;data analysis module;data collection;elderly homes application;m-health monitoring services;m-health monitoring system;medical resource assignment;medical services;mobile network;monitoring data;person health condition monitoring;personalized health data processing;pervasive health monitoring;public medical resources;resource allocation;service composition method;Biomedical monitoring;Cloud computing;Diseases;Hospitals;Monitoring;Senior citizens;cloud computing platform;hosptial information system;m-Health monitoring system;multiple tenants;service composition},
 note = {from: ieee.bib},
 pages = {45--50},
 title = {Architecture of M-Health Monitoring System Based on Cloud Computing for Elderly Homes Application},
 year = {2014}
}

@inproceedings{6999236,
 abstract = {Most industrialized countries are facing an important aging of their population. To meet needs of seniors and meet economic and sociological challenges such as the pressure on health support services for semi-autonomous persons, smart home technology is considered by many researchers as a promising potential solution. This approach proposes to use ubiquitous sensors hidden in the environment for monitoring and detecting behavioral abnormalities associated with cognitive deficits and then do a proper guidance by giving hints using many kinds of effectors (light, sound, screen, etc.). In a smart environment, assistive system is an important technology in order to fulfill the need to provide support in living environment for people with cognitive deficit. In this paper, we present a new affordable multi-agents assistance system who exploits qualitative spatial reasoning from a combination of RFID localization of everyday life objects and load signature of appliances. We also present promising results of experiments conducted on this new assistance system with real case scenarios of daily living.},
 author = {Bilodeau, J. S. and Fortin-Simard, D. and Gaboury, S. and Bouchard, B. and Bouzouane, A.},

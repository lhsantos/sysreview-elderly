@inproceedings{6165450,
 abstract = {This paper describes the development of a computer-based serious game to enable older individuals to practice Tai Chi at home on their own. The player plays the game by imitating Tai Chi movements presented by a virtual instructor on the screen. The proposed system is decomposed into two modules. The first module is the game design, i.e., the process of recording an instructor training Tai Chi. Acquired data are used to create gesture templates and a virtual instructor. The second module is the game play in which the player attempts to mimic the virtual instructor. Gestures are measured in real-time and then compared with the prerecorded Tai Chi gesture template corresponding to the displayed movement. Visual feedback indicates how well the player imitated the instructor. The proposed system is not designed to classify gestures but to evaluate the similarity of a given gesture with a gesture template. The Longest Common Sub-Sequence (LCSS) method is applied to compute such similarity. The proposed approach (1) facilitates the design of assessment tools in which the user has to follow a sequence of predefined movements and (2) applicable to other domains, such as telerehabilitation.},
 author = {de Morais, W. O. and Wickström, N.},
 booktitle = {Serious Games and Applications for Health (SeGAH), 2011 IEEE 1st International Conference on},
 doi = {10.1109/SeGAH.2011.6165450},
 keyword = {computer based training;computer games;handicapped aids;patient rehabilitation;Tai Chi training;computer-based serious game;elderly people;gesture templates;longest common sub-sequence method;telerehabilitation;virtual instructor;visual feedback;Acceleration;Games;Hidden Markov models;Image segmentation;Protocols;Sensors;Training;Serious Games;Tai Chi;fall prevention;gesture evaluation;movement analysis;wearable inertial measurement units},
 note = {from: ieee.bib},
 pages = {1--8},
 title = {A serious computer game to assist Tai Chi training for the elderly},
 year = {2011}
}

@inproceedings{6169847,
 abstract = {Nowadays, recognizing human activities is an important subject; it is exploited widely and applied to many fields in real-life, especially health care or context aware application. Research achievements are mainly focused on activities of daily living which are useful for suggesting advises to health care applications. Falling event is one of the biggest risks to the health and well being of the elderly especially in independent living because falling accidents may be caused from heart attack. Recognizing this activity still remains in difficult research area. Many systems which equip wearable sensors have been proposed but they are not useful if users forget to wear the clothes or lack ability to adapt themselves to mobile systems without specific wearable sensors. In this paper, we develop novel method based on analyzing the change of acceleration, orientation when the fall occurs. In this study, we recruit five volunteers in our experiment including various fall categories. The results are effective for recognizing fall activity. Our system is implemented on Google Android smart phone which already plugged accelerometer and orientation sensors. The popular phone is used to get data from accelerometer and results show the feasibility of our method and contribute significantly to fall detection in Health care.},
 author = {Vo, Q. V. and Lee, G. and Choi, D.},
 booktitle = {Computing and Communication Technologies, Research, Innovation, and Vision for the Future (RIVF), 2012 IEEE RIVF International Conference on},
 doi = {10.1109/rivf.2012.6169847},
 keyword = {acceleration measurement;biomechanics;biomedical communication;health care;medical computing;patient monitoring;smart phones;ubiquitous computing;Google Android smart phone;acceleration measurement;accelerometer sensor;context aware application;fall detection;health care application;orientation sensor;smart phone technology;Acceleration;Accelerometers;Humans;Legged locomotion;Sensors;Smart phones},
 note = {from: ieee.bib},

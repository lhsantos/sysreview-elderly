 title = {Designing home care reminder systems: Lessons learned through co-design with older users},
 year = {2012}
}

@inproceedings{6240372,
 abstract = {A novel approach to performing unsupervised mobility assessment tests in domestic environments is presented. As a part of the aTUG concept the approach is based on the idea to segment assessment tests into components made up of recurring movement patterns which are measured independently by use of ambient sensor technologies. Quality criteria are defined which compute a score of eligibility for usage of sensor data to assess a certain test component. Valid component measurements are recombined to complete assessment tests according to a technical assessment test description defining the flow of segments and their constraints. An experiment has been conducted within a field trial with five elderly people aged 64-84 years over five weeks. The flats of all people were equipped with home automation (HA) sensors. A laser range scanner (LRS) was placed in one flat. Results from the fully-equipped flat show that the presented quality criteria are suitable to select LRS measurements according to their eligibility to assess a certain component. HA sensors and the LRS were used to compute a self-selected gait velocity of 0.71m/s unsupervised at home. TUG using the aTUG apparatus and a stopwatch was used as clinical reference data yielding a mean gait velocity of 1.18m/s. For the described setting a difference of 0.47m/s between capacity and performance in gait velocity was found.},
 author = {Frenken, T. and Lipprandt, M. and Brell, M. and Gövercin, M. and Wegel, S. and Steinhagen-Thiessen, E. and Hein, A.},
 booktitle = {2012 6th International Conference on Pervasive Computing Technologies for Healthcare (PervasiveHealth) and Workshops},
 doi = {10.4108/icst.pervasivehealth.2012.248614},
 issn = {2153-1633},
 keyword = {home automation;medical computing;optical scanners;unsupervised learning;aTUG apparatus;ambient sensor technologies;clinical reference data;domestic environments;elderly people;home automation sensors;laser range scanner;movement patterns;self-selected gait velocity;sensor data;technical assessment test description;unsupervised mobility assessment tests;Quantum cascade lasers;Radio frequency;Subspace constraints;Domestic Assessment;Force Sensors;LIDAR;Laser Range Scanner;Mobility Assessment;Sensor Fusion;Timed Up and Go (TUG);aTUG},
 note = {from: ieee.bib},
 pages = {131--138},
 title = {Novel approach to unsupervised mobility assessment tests: Field trial for aTUG},
 year = {2012}
}

@inproceedings{6240378,
 abstract = {Falling is a common health problem for elderly. It is reported that about 12 million adults 65 and older fall each year in the United States. To address this problem, at the Center for Eldercare and Rehabilitation Technologies in the University of Missouri we are investigating multiple fall detection systems. In this paper, we present an automatic fall detection system called VAMPIR based on a vertical array of multiple passive infrared (PIR) sensors. PIR sensors provide an inexpensive way to recognize human activity based on its infrared signature. To differentiate between falls and other human activities such as walking, sitting on a chair, bending over etc., we used a pattern recognition algorithm based on hidden Markov models (HMM). We obtained encouraging classification results on a pilot dataset that contained 42 falls and multiple non-fall human activities performed by trained stunt actors.},
 author = {Popescu, M. and Hotrabhavananda, B. and Moore, M. and Skubic, M.},
 booktitle = {2012 6th International Conference on Pervasive Computing Technologies for Healthcare (PervasiveHealth) and Workshops},

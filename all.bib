@inproceedings{7067097,
 abstract = {In this paper we present a Health Emergency Event Notification system, which is intended to support the monitorization of the elderly health care; focusing on seamless service mobility paradigm. The system is composed by five features: i) the vital signs recording, ii) emergency contact phone numbers register (for SMS - Short Message Service and phone calls), iii) monitorization profile creation, iv) messages notification management, and v) local mobile storage service. Our architecture was designed to drive information through the WiFi / Cellular network from a WSN (Wireless Sensor Network) even when there is not real-time connection. This proposal considers as restriction connectivity costs. The proposal was designed considering the Scenario-Based Design methodology. In formation is related to a project conducted a long one year in Harmony house. Meanwhile, we were designing the communication infrastructure. Our architecture was evaluated by 2 caregivers and 2 nurses from Harmony house. The results showed high scores in analyzed aspects: perceived ease of use, usefulness and intended use. We conclude that in order to complement and enhance the initial design, it is necessary to carry out an in-site evaluation. Our future work includes to analyze the architecture performance under different connectivity scenarios.},
 author = {Beltran, J. A. G. and Leon, M. C. and Martınez, A. I. G. and Hipolito, J. I. N.},

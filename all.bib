 note = {from: ieee.bib},
 pages = {2246--2249},
 title = {Wearable sensor networks supported by mobile devices for fall detection},
 year = {2014}
}

@inproceedings{6997017,
 abstract = {M-Health monitoring services have attracted more and more attentions especially for senior people. Many people living in elderly homes hope their health conditions to be monitored pervasively so that they could be warned before they get serious diseases. Comparing to traditional medical services which are usually provided within hospitals, m-Health monitoring faces more challenges in personalized health data processing from both views of supporting technology and application mode. In order to achieve personalized and high quality of health monitoring enabled by the new technologies, such as mobile network and cloud computing, in this paper, a framework of m-Health monitoring system based on cloud computing platform (Could-MHMS) is firstly proposed and designed to implement pervasive health monitoring. Furthermore, the modules of the framework are discussed, which are cloud data storage module, data analysis module, and resource allocation & assignment module, respectively. In data storage module, monitoring data are stored in multiple tenant spaces. Data analysis module uses domain knowledge to assess the monitored persons' health conditions. Service composition method is designed to dynamically allocate and assign public medical resources. These three modules cooperate to implement the core functions during the process of health monitoring which are data collection, data storage, data analysis, and medical resources assignment. Finally, we demonstrate the application of our architecture in the environment of elderly homes and show the usability of our method.},
 author = {Xu, B. and Xu, L. and Cai, H. and Jiang, L.},

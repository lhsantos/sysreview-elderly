 doi = {10.1109/MESA.2014.6935631},
 keyword = {assisted living;data acquisition;data analysis;inference mechanisms;object-oriented programming;ontologies (artificial intelligence);pattern matching;query processing;semantic networks;AAL systems;AALISABETH project;Region Marche Government;ambient assisted living;data analysis;data collections;data preprocess;data repository;data type;data value;device name;domain ontology;elderly;event-based human activity recognition;health status monitoring;heterogeneous sources;living conditions;ontology-based methodology;pattern matching process;reasoning techniques;semantic query;sensor devices;sensors networks;software component;temporal analysis;temporal event sequences recognition;timestamp;Cognition;Databases;Engines;Java;OWL;Ontologies;Semantics;Complex Event Processing (CEP);OntoAALISABETH Domain Ontology;Pattern Recognition;Rule-based system;Semantic Reasoning},
 note = {from: ieee.bib},

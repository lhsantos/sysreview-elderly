@inproceedings{6720707,
 abstract = {The increasing number of elderly citizens in our society increases the strain on traditional healthcare systems' resources. Ambient Assisted Living solutions seek to reduce the impact of this trend as well as provide a way for the elderly to continue living in their home environment as long as possible with a good quality of life and independence. However, due to the diverse nature of existing products and services, ensuring interoperability between them is important to provide flexible and adequate solutions for an individual's particular needs. The goal of this work is to propose a solution for this problem using Electronic Health Records and health information standards. For this purpose, a repository was implemented to store records in Extensible Markup Language format following the openEHR standard, as well as an application to create, manage and query these records. An interface engine was used to provide message transformation capabilities between different standards and openEHR. The results gathered from an evaluation of this work show that such a system provides fast insertion of information and reasonable querying times for single records. Also, the message transformation capabilities developed in conjunction with the storage mechanism provide an important step towards solving the interoperability problem.},
 author = {Osório, E. and Ferreira, L. and Abreu, R. and Sousa, F.},
 booktitle = {e-Health Networking, Applications Services (Healthcom), 2013 IEEE 15th International Conference on},
 doi = {10.1109/HealthCom.2013.6720707},
 keyword = {XML;assisted living;electronic health records;geriatrics;home computing;human computer interaction;medical computing;open systems;query processing;OpenEHR;ambient assisted living;elderly citizens;electronic health records;extensible markup language format;health information standards;healthcare system resources;home environment;interface engine;interoperability problem;message transformation capabilities;quality-of-life;querying times;storage mechanism;Conferences;Electronic medical records;Interoperability;Monitoring;Prototypes;Senior citizens;Standards},

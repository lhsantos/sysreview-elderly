}

@inproceedings{7336998,
 abstract = {Ambient Assisted Living (AAL) is a research field whose main objective is to develop health monitoring systems for elderly patients at home, increasing the degree of patient independence during chronic diseases treatment. AAL systems are composed by several technologies: medical sensors and actuators, computer and mobile devices, Wireress Sensors Networks (WSN), computer networks, software infrastructure and applications. A comprehensive software infrastructure for AAL systems should use these technologies in an integrated manner and be able to cover scenarios involving several patient mobility level, patient locations, and levels of physical and cognitive abilities. The integration of AAL with the Internet of Things (IoT) technologies expands the scope of these systems with the ability to connect various sensors and actuators, integrating them into a single network that serves the purpose of patient health monitoring. In this context, the contribution of this paper is to present a comprehensive Cloud based IoT softwre infrastructure for Ambient Assisted Living. The main components of this software infrastructure are the M-Hub and the SDDL middleware. Preliminary results show that the proposed infrastructure has the potential to be applied in a wide range of health monitoring scenarios.},
 author = {Gomes, B. and Muniz, L. and Silva, F. J. da Silva e and Ríos, L. E. T. and Endler, M.},
 booktitle = {Cloud Technologies and Applications (CloudTech), 2015 International Conference on},
 doi = {10.1109/CloudTech.2015.7336998},
 keyword = {Internet of Things;assisted living;cloud computing;geriatrics;home computing;middleware;patient treatment;AAL;Internet of Things;M-Hub;SDDL middleware;ambient assisted living;chronic diseases treatment;cloud-based IoT software infrastructure;elderly patient;home environment;patient health monitoring system;Cloud computing;Intelligent sensors;Mobile communication;Monitoring;AAL systems;Ambient Assisted Living;Cloud-based IoT middleware;Health Monitoring;Internet of Mobile Things;M-Hub},
 note = {from: ieee.bib},
 pages = {1--8},
 title = {A comprehensive cloud-based IoT software infrastructure for Ambient Assisted Living},
 year = {2015}
}


}

@inproceedings{6521157,
 abstract = {This study incorporated a virtual orchid gardening game as a case of Orange Technology game (OTG) to qualitatively study the factors influencing senior user acceptance and their causal relationships. In this study, we first conduct a panel discussion with 15 participants to come out general issues; then conduct user interviews with 23 senior users to evaluate the significance of each issue, and to explore their relationships and make propositions. The results show that knowledge learning and social interaction have positive effects on both self-efficacy and hedonic experience, self-efficacy also has a positive effect on hedonic experience, and hedonic experience will have a direct impact on behavior intention to use OTG system. Consequently, we summarize above propositions into a conceptual model, named as seniors' game-technology acceptance model (SGTAM).},
 author = {Lin, M. H. and Chen, S. H.},

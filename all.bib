@inproceedings{5482236,
 abstract = {Active ageing, through a balanced combination of leisure and social interaction with continued work involvement, is central to meeting older citizens expectations, and maintaining their mental and physical health. Application of the collaborative networks paradigm, and the new generation of collaboration-support platforms and tools, can offer a promising approach to supporting active ageing, and can facilitate better use of the talents and potential of retired or retiring senior professionals. This paper introduces the results of a roadmapping initiative addressing the implementation of a new vision for extending professional active life. To support this vision, a strategic research plan for the development of a new digital ecosystem, covering the social, organizational, and technological perspectives, is proposed. A large number of stakeholders coming from different backgrounds have contributed to the design and validation of this roadmap.},
 author = {Camarinha-Matos, L. M. and Cura, A. del and Afsarmanesh, H. and Playfoot, J.},

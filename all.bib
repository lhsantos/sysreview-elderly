@inproceedings{6708062,
 abstract = {The ageing population that becomes a predominant demographic feature in most countries raises new challenges in healthcare. Over the past years, the focus was shifted from institutional-based healthcare to automatic healthcare systems by leveraging the sensing technologies and the communication infrastructures. The aim is to provide the elderly and people with physical and/or cognitive impairment with intelligent and assistive health services in a pervasive, timely and cost effective manner. In this paper we propose an integrated healthcare framework that enables pervasive and personalized health services. The proposed framework is based on a M2M infrastructure towards high scalability and easy service deployment. In addition, using ontology as information model provides deep semantic and expressiveness in the domain knowledge enabling thus more interoperability and service personalization and automation through logic-based reasoning.},
 author = {Chellouche, S. A. and Chalouf, M. A. and Lemlouma, T.},

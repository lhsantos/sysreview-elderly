    author = {Marco Mazzù and Simonetta Scalvini and Amerigo Giordano and Enrico Frumento and Hannah Wells and Kees Lokhorst and Fulvio Glisenti},
    journal = {{J Telemed Telecare}},
    volume = {14},
    number = {3},

    journal = {{Environ. Res.}},
    volume = {126},
    year = {2013},
    month = oct,
    pages = {66--75},
    abstract = {Synoptic circulation patterns (large-scale weather systems) affect ambient levels of air pollution, as well as the relationship between air pollution and human health.},
    pubmed = {24012249},

    author = {E Aga and E Samoli and G Touloumi and H R Anderson and E Cadum and B Forsberg and P Goodman and A Goren and F Kotesovec and B Kriz and M Macarol-Hiti and S Medina and A Paldy and C Schindler and J Sunyer and P Tittanen and B Wojtyniak and D Zmirou and J Schwartz and K Katsouyanni},
    journal = {{Eur Respir J Suppl}},
